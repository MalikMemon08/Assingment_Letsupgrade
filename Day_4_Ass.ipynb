      "source": [
        "n = int(input('Enter a no.: '))\n",
        "def factorial(no):\n",
        "  fact = 1\n",
        "  while 1 < no:\n",
        "    fact = fact * no\n",
        "    no -= 1\n",
        "  return fact\n",
        "print('Factorial Of:',n, 'is', factorial(n))"
      ],
